{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": "always",
		"source.organizeImports": "always"
	},
	"prettier.tabWidth": 2,
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"typescript.tsdk": "node_modules\\typescript\\lib",
	"i18n-ally.localesPaths": ["messages"],
	"workbench.colorCustomizations": {
		"activityBar.activeBackground": "#8f0a2d",
		"activityBar.background": "#8f0a2d",
		"activityBar.foreground": "#e7e7e7",
		"activityBar.inactiveForeground": "#e7e7e799",
		"activityBarBadge.background": "#000000",
		"activityBarBadge.foreground": "#e7e7e7",
		"commandCenter.border": "#e7e7e799",
		"sash.hoverBorder": "#8f0a2d",
		"statusBar.background": "#60071e",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#8f0a2d",
		"statusBarItem.remoteBackground": "#60071e",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"titleBar.activeBackground": "#60071e",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#60071e99",
		"titleBar.inactiveForeground": "#e7e7e799"
	},
	"peacock.color": "#60071e"
}
